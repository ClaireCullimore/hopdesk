<div class="container">
  <h2>My Bookings</h2>
  <% @bookings.each do |booking| %>
    <%= booking.workspace.name %>
    <%= booking.workspace.postcode %>
    <%= booking.person_count %>
    <%= booking.start_time %>
    <%= booking.end_time %>
    <%= link_to 'Edit booking', edit_workspace_booking_path(booking.workspace, booking) %>
  <% end %>
</div>
